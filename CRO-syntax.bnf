@@ ::= <statement> (@@ | <None>)
<state> ::= <affect> | <print>
<affect> ::= var "=" <expr> ";"
<print> ::= "PRINT" <expr>;
<expr> ::= var | num | <add> | <sub> | <div> | <mul> | "(" <expr> ")"
<add> ::= <expr> "+" <expr>
<sub> ::= <expr> "-" <expr>
<div> ::= <expr> "/" <expr>
<mul> ::= <expr> "*" <expr>

num ::= [0-9]+
var ::= [a-z]+[0-9]*

